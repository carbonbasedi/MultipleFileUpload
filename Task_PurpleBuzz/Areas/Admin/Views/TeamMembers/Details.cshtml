@using Task_PurpleBuzz.Areas.Admin.ViewModels.TeamMembers;
@model TeamMemberDetailsVM

<div class="m-4 form-group">
	<a class="btn btn-primary" asp-action="index" asp-controller="teammembers">Return to list</a>
	<h1>Name</h1>
	<p>@Model.Name</p>
	<br />
	<h1>Occupancy</h1>
	<p>@Model.Occupancy</p>
	<br />
	<h1>Image Link</h1>
	<p>@Model.ImgPath</p>
	<br />
	<h1>Created At</h1>
	<p>@Model.CreatedAt.ToString("dd.MM.yyyy HH:mm:ss")</p>
	<br />
	<h1>Modified At</h1>
	<p>@Model.ModifiedAt?.ToString("dd.MM.yyyy HH:mm:ss")</p>
</div>